--CONSULTA1--
select PERIODO AS "AÃ±o electivo", NOMBRE_CURSO as "Curso", count(DISTINCT ESTUDIANTE) as "Cantidad de estudiantes" from ESTUDIANTE
inner join MATRICULA on ESTUDIANTE.COD_ESTUDIANTE = MATRICULA.COD_ESTUDIANTE inner join PERIODO on MATRICULA.COD_PERIODO = PERIODO.COD_PERIODO 
inner join CLASE on MATRICULA.COD_CLASE = CLASE.COD_CLASE inner join CURSO on CLASE.COD_CURSO = CURSO.COD_CURSO GROUP BY PERIODO, NOMBRE_CURSO
--CONSULTA2--
SELECT PERIODO.PERIODO as "Periodo electivo", count((nota_primerparcial)+(nota_segundoparcial)) as "Reprobados", MATERIA.NOMBRE_MATERIA as "Materia"
from Calificacion inner join CLASE on Calificacion.COD_CLASE = CLASE.COD_CLASE inner join PERIODO on CLASE.COD_PERIODO = PERIODO.COD_PERIODO 
inner join MATERIA on CLASE.COD_MATERIA = MATERIA.COD_MATERIA where ((nota_primerparcial)+(nota_segundoparcial)) < 14 GROUP BY PERIODO, NOMBRE_MATERIA
--CONSULTA3--
SELECT PERIODO.PERIODO as "Periodo electivo", ESTUDIANTE.NOMBRE_ESTUDIANTE as "Estudiante", CURSO.NOMBRE_CURSO as "Curso", 
NOMBRE_MATERIA as "Materia", CALIFICACION.NOTA_PRIMERPARCIAL as "Primer Parcial", CALIFICACION.NOTA_SEGUNDOPARCIAL as "Segundo Parcial" 
from CALIFICACION inner join ESTUDIANTE on ESTUDIANTE.COD_ESTUDIANTE = CALIFICACION.COD_ESTUDIANTE 
inner join CLASE on CLASE.COD_CLASE = CALIFICACION.COD_CLASE inner join PERIODO on PERIODO.COD_PERIODO = CLASE.COD_PERIODO 
inner join CURSO on CURSO.COD_CURSO = CLASE.COD_CURSO inner join MATERIA on MATERIA.COD_MATERIA = CLASE.COD_MATERIA WHERE PERIODO = '2022-1'
--CONSULTA4--
select DISTINCT PERIODO.PERIODO, ESTUDIANTE.COD_ESTUDIANTE, DNI_ESTUDIANTE, NOMBRE_ESTUDIANTE, PROVINCIA_ESTUDIANTE, CURSO.NOMBRE_CURSO, SECCION
from PERIODO inner join MATRICULA on PERIODO.COD_PERIODO = MATRICULA.COD_PERIODO 
inner join ESTUDIANTE ON ESTUDIANTE.COD_ESTUDIANTE = MATRICULA.COD_ESTUDIANTE inner join CLASE on CLASE.COD_CLASE = MATRICULA.COD_CLASE 
inner join CURSO on CURSO.COD_CURSO = CLASE.COD_CURSO WHERE PERIODO = '2022-2'